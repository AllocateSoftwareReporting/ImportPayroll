  <DataSets>
    <DataSet Name="import_EMPBA">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT DISTINCT 
	'import_EMPBA' AS record_key,
	'1' as record_no ,
	T.emp_no,
	s.payroll_ref as leave_type,
	T.day_date,
	T.entitlement,
	T.pro_rata,
	T.pro_rata_taken,
	T.anniversary_date	
FROM (
	SELECT LEFT(Rtrim(e.Column02),20) as emp_no,
		'IMP-AL' as leave_group_type_code,
		CASE
			WHEN e.Column29 = '' THEN
				CASE 
					WHEN e.Column30 = '1901-01-01' THEN '1901-01-01' 
					ELSE CONVERT(VARCHAR(10), e.Column30, 121)
				END
			ELSE CONVERT(VARCHAR(10), DATEADD(dd, 1, e.Column29), 121)
		END as day_date,
		CASE
			WHEN e.Column31 &lt;&gt; '' THEN e.Column31
			ELSE '0'
		END as entitlement,
		CASE
			WHEN e.Column32 &lt;&gt; '' THEN e.Column32
			ELSE '0'
		END as pro_rata,
		CASE
			WHEN e.Column33 &lt;&gt; '' THEN e.Column33
			ELSE '0'
		END as pro_rata_taken,
		e.Column34 as anniversary_date
	FROM gi_Employee e
	WHERE e.Column46 not in ('FN01', 'FN03', 'FN04')

	UNION ALL

	SELECT LEFT(Rtrim(e.Column02),20) as emp_no,
		'IMP-SL' as leave_group_type_code,
		CASE
			WHEN e.Column29 = '' THEN
				CASE 
					WHEN e.Column35 = '' THEN '01/01/1901' 
					ELSE CONVERT(VARCHAR(10), e.Column35, 121)
				END
			ELSE CONVERT(VARCHAR(10), DateAdd(dd, 1, e.Column29), 121)
		END as day_date,
		CASE
			WHEN e.Column36 &lt;&gt; '' THEN e.Column36
			ELSE '0'
		END as entitlement,
		CASE
			WHEN e.Column37 &lt;&gt; '' THEN e.Column37
			ELSE '0'
		END as pro_rata,
		CASE
			WHEN e.Column38 &lt;&gt; '' THEN e.Column38
			ELSE '0'
		END as pro_rata_taken,
		e.Column39 as anniversary_date
	FROM gi_Employee e
	WHERE e.Column46 not in ('FN01', 'FN03', 'FN04')

	UNION ALL

	SELECT LEFT(Rtrim(e.Column02),20) as emp_no,
		'IMP-LSL' as leave_group_type_code,
		CASE
			WHEN e.Column29 = '' THEN
				CASE 
					WHEN e.Column40 = '1901-01-01' THEN '1901-01-01' 
					ELSE CONVERT(VARCHAR(10), e.Column40, 121)
				END
			ELSE CONVERT(VARCHAR(10), DateAdd(dd, 1, e.Column29), 121)
		END as day_date,
		CASE
			WHEN e.Column41 &lt;&gt; '' THEN e.Column41
			ELSE '0'
		END as entitlement,
		'0' as pro_rata,
		CASE
			WHEN e.Column42 &lt;&gt; '' THEN e.Column42
			ELSE '0'
		END as pro_rata_taken,
		e.Column43 as anniversary_date
	FROM gi_Employee e
	WHERE e.Column46 not in ('FN01', 'FN03', 'FN04')

	UNION ALL

	SELECT LEFT(Rtrim(e.Column02),20) as emp_no,
		'IMP-PHL' as leave_group_type_code,
		CASE 
			WHEN e.Column29 = '1900-01-01' THEN '1901-01-01' 
			ELSE CONVERT(VARCHAR(10), e.Column29, 121) 
		END as day_date,
		CASE
			WHEN e.Column50 &lt;&gt; '' THEN e.Column50
			ELSE '0'
		END as entitlement,
		'0' as pro_rata,
		'0' as pro_rata_taken,
		e.Column51 as anniversary_date
	FROM gi_Employee e
	WHERE e.Column46 not in ('FN01', 'FN03', 'FN04')

	UNION ALL

	SELECT RTrim(e.Column02) as emp_no,
		CASE elc.Column04 
			WHEN '48/52' THEN 'IMP-4852'
			WHEN 'T' THEN 'IMP-TIL'
			WHEN 'LIEU' THEN 'IMP-TIL'
		END as leave_group_type_code,
		CASE 
			WHEN e.Column29 = '1900-01-01' THEN '1901-01-01' 
			ELSE CONVERT(VARCHAR(10), e.Column29, 121) 
		END as day_date,
		CASE 
			WHEN elc.Column03 &lt;&gt; '' THEN elc.Column03 
			ELSE '0'
		END as entitlement,
		'0' as pro_rata,
		'0' as pro_rata_taken,
		e.Column45 as anniversary_date
	FROM gi_Employee e
	LEFT OUTER JOIN gi_EmpLeave elc ON e.Column02 = elc.Column02
	WHERE elc.Column04 IN ('48/52', 'T', 'LIEU')
	AND e.Column46 not in ('FN01', 'FN03', 'FN04')
	
	UNION ALL
	
	SELECT RTrim(e.Column02) as emp_no,
		'IMP-ADO' as leave_group_type_code,
		CASE 
			WHEN e.Column29 = '1901-01-01' THEN '1901-01-01' 
			ELSE CONVERT(VARCHAR(10), DateAdd(dd, 1, e.Column29), 121) 
		END as day_date,
		CASE 
			WHEN elc.Column03 &lt;&gt; '' THEN elc.Column03 
			ELSE '0'
		END as entitlement,
		'0' as pro_rata,
		'0' as pro_rata_taken,
		e.Column45 as anniversary_date
	FROM gi_Employee e
	LEFT OUTER JOIN gi_EmpLeave elc ON e.Column02 = elc.Column02
	WHERE elc.Column04 = 'ADO'
	AND e.Column46 not in ('FN01', 'FN03', 'FN04')

	)T
INNER JOIN leave_group_type lgt ON T.leave_group_type_code = lgt.leave_group_type_code
INNER JOIN leave_group lg ON lg.leave_group_type_id = lgt.leave_group_type_id 
						 AND GetDate() BETWEEN lg.eff_from_date AND lg.eff_to_date
INNER JOIN shift s ON lg.shift_id = s.shift_id
ORDER BY T.emp_no, s.payroll_ref</CommandText>
