  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT DISTINCT 'KEYDATE' AS KEYDATE,
	A.emp_no, 
	CONVERT(VARCHAR(10), A.eff_from_date, 121) as eff_from_date,
	CONVERT(VARCHAR(10), CASE
							WHEN ISNULL(DATEADD(DD, -1, MIN(B.eff_from_date)), '2049-01-01 00:00:00.000') &gt; ISNULL(et.Column02, '2049-01-01 00:00:00.000') 
								THEN ISNULL(et.Column02, '2049-01-01 00:00:00.000')
							ELSE ISNULL(DATEADD(DD, -1, MIN(B.eff_from_date)), '2049-01-01 00:00:00.000') 
						END, 121) as eff_to_date
FROM	(SELECT ep.Column01 as emp_no, 
			ep.Column02 as eff_from_date
		FROM gi_EMPOS ep
		INNER JOIN gi_EMDET ed ON ed.Column01 = ep.Column01
										AND ed.Column06 &lt;= ep.column02
		UNION ALL
		SELECT es.Column01 as emp_no,
			es.Column02 as eff_from_date
		FROM gi_EMSAL es
		INNER JOIN gi_EMDET ed ON ed.Column01 = es.Column01
										AND ed.Column06 &lt;= es.Column02
		)A
LEFT JOIN (SELECT ep.Column01 as emp_no, 
			ep.Column02 as eff_from_date
			FROM gi_EMPOS ep
			INNER JOIN gi_EMDET ed ON ed.Column01 = ep.Column01
											AND ed.Column06 &lt;= ep.column02
			UNION ALL
			SELECT es.Column01 as emp_no,
				es.Column02 as eff_from_date
			FROM gi_EMSAL es
			INNER JOIN gi_EMDET ed ON ed.Column01 = es.Column01
											AND ed.Column06 &lt;= es.Column02
			)B ON B.emp_no = A.emp_no
			  AND B.eff_from_date &gt; A.eff_from_date
LEFT JOIN gi_EMTER et ON et.Column01 = A.emp_no
WHERE ISNULL(et.Column02, '2049-01-01 00:00:00.000') &gt; A.eff_from_date							
GROUP BY A.emp_no, A.eff_from_date, et.Column02
ORDER BY emp_no, eff_from_date</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
