  <DataSets>
    <DataSet Name="import_EMPAT">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT 'import_EMPAT' AS record_key
	,'1' AS record_no
	,emp_no
	,attribute
	,value
	,eff_from_date
	,CASE
		WHEN eff_to_date &gt; ISNULL(et.Column03, '2049-01-01') THEN CONVERT(DATETIME, et.Column03)
		ELSE CONVERT(DATETIME, eff_to_date)
	END as eff_to_date
FROM	(SELECT ea.Column02 as emp_no, 
			ea.Column05 as attribute, 
			(CASE	
				WHEN CONVERT(float, ea.Column06) &gt; 0 THEN CONVERT(float, ea.Column06)
				ELSE CONVERT(float, ea.Column07)
			END) as value, 
			CONVERT(DATETIME, ea.Column03) as eff_from_date, 
			CASE
				WHEN ea.Column04 = '' THEN CONVERT(DATETIME, '2049-01-01')
				ELSE CONVERT(DATETIME, ea.Column04)
			END as eff_to_date
		FROM gi_EMP_DETAIL ed
		INNER JOIN gi_EMP_ALLOWANCE ea ON ed.Column02 = ea.Column02
		WHERE ed.Column07 &lt;= ea.Column03 
		AND	ea.Column05 &lt;&gt; '' 
		AND (CONVERT(DECIMAL(12,2), ea.Column06) &gt; '0' OR CONVERT(DECIMAL(15,5), ea.Column07) &gt; '0')

		UNION ALL

		SELECT ep.Column02 as emp_no,
			eat.payroll_ref as attribute, 
			ep.Column05  as value, 
			CONVERT(DATETIME, ep.Column03) as eff_from_date, 
			CASE	
				WHEN ep.Column04 = '' THEN CONVERT(DATETIME, '2049-01-01')
				ELSE CONVERT(DATETIME, ep.Column04) 
			END as eff_to_date
		FROM gi_EMP_DETAIL ed
		INNER JOIN gi_EMP_POSITION ep ON ed.Column02 = ep.Column02
		JOIN emp_attribute_type eat ON eat.emp_attribute_type_code = 'LG'
		WHERE ed.Column07 &lt;= ep.Column03
		AND	ed.Column07 &lt; CASE	
								WHEN ep.Column04 = '' THEN '2049-01-01'
								ELSE ep.Column04
							END 
		AND	eat.emp_attribute_type_code = 'LG'

		UNION ALL

		SELECT ep.Column02 as emp_no,
			eat.payroll_ref as attribute , 
			ep.Column05 as value, 
			GETDATE() as eff_from_date, 
			'2049-01-01' as eff_to_date
		FROM gi_EMP_PAY ep 
		INNER JOIN emp_attribute_type eat ON eat.emp_attribute_type_code = 'LDB'
		)A
LEFT JOIN gi_EMP_TERMINATION et ON et.Column02 = A.emp_no
WHERE CASE
		WHEN eff_to_date &gt; ISNULL(et.Column03, '2049-01-01') THEN CONVERT(DATETIME, et.Column03)
		ELSE CONVERT(DATETIME, eff_to_date)
	END &gt; DATEADD(MM, -12, GETDATE())</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
