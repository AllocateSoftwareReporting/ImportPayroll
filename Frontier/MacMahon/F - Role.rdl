  <DataSets>
    <DataSet Name="import_ROLE">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT 'import_ROLE' AS record_key
	, '1' AS record_no
	, Column14 + '-' + Column02 AS payroll_ref
	, Column14 AS area
	, Column02 AS role_code
	, Column03 + ' - ' + Column02 AS role_desc
	, ISNULL((SELECT jr.allocation_order
			  FROM	job_role jr
			  INNER JOIN area a ON jr.area_id = a.area_id
			  WHERE Column12 = a.payroll_ref
			  AND Column12 + '-' + Column02 = jr.payroll_ref
			  ),'1') AS allocation_order
	, '' AS gl_code 
FROM gi_Position_Detail</CommandText>
      </Query>
      <Fields>
        <Field Name="record_key">
          <DataField>record_key</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="record_no">
