  <DataSets>
    <DataSet Name="import_AREA">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT 'import_AREA' AS record_key,
	'1' AS record_no, 
	o.Column02 as area, 
	LEFT(o.Column02, 9) as area_code, 
	o.Column03 as area_desc, 
	o.Column04 as area_level, 
	'ALL' as parent_area, 
	'' as gl_code, 
	1 as order_by
FROM gi_ORG_STRUCTURE o
WHERE o.Column04 = 2 
AND	o.Column02 NOT IN (SELECT a.payroll_ref
					FROM area a 
					WHERE a.area_code = LEFT(o.Column02, 9) 
					AND	a.area_desc = o.Column03
					) 

UNION ALL

SELECT 'import_AREA' AS record_key,
	'1' AS record_no, 
	o1.Column02 + o2.Column02 as area, 
	LEFT(o1.Column02 + o2.Column02, 9) as area_code, 
	o2.Column02 + ' - ' + o1.Column03 as area_desc, 
	3 as area_level, 
	o1.Column02 as parent_area, 
	'' as gl_code, 
	1 as order_by
FROM gi_ORG_STRUCTURE o1
INNER JOIN gi_ORG_STRUCTURE o2 ON o2.Column04 = 3 								
WHERE	o1.Column04 = 2 
AND LEFT(o1.Column02 + o2.Column02, 9) NOT IN (SELECT a.area_code
												FROM area a 
												WHERE area_code = (o1.Column02 + o2.Column02)
												AND	area_desc = (o2.Column02 + ' - ' + o1.Column03)
												)

UNION ALL

SELECT 'import_AREA' AS record_key,
	'1' AS record_no, 
	rtrim(x.Project) as area, 
	left(rtrim(x.Project), 9) as area_code, 
	rtrim(x.Project) + '-' + rtrim(o.Column03) as area_desc, 
	'5' as area_level, 
	rtrim(x.Parent) as parent_area, 
	'' as gl_code, 
	1 as order_by
FROM gi_ORG_STRUCTURE o, 
	(SELECT MAX(Z.counter) 'MaxCounter',  
			Z.Project  
	FROM (SELECT count(Column02) as 'Counter',  
				Column14 as 'Project',  
				Column11 + Column12 as 'Parent'  
			FROM gi_POSITION_DETAIL 
			GROUP BY Column14, Column11, Column12
			) Z 
		GROUP BY Z.Project 
	)y  
	JOIN 
	(SELECT	count(Column02) as 'Counter',  
			Column14 as 'Project',  
			Column11 + Column12 as 'Parent'  
		FROM gi_POSITION_DETAIL  
		GROUP BY Column14, Column11, Column12 
	) x ON y.MaxCounter = x.Counter AND y.Project = x.Project 
WHERE x.Project = o.Column02 
