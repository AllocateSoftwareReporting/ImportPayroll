  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT record_key,
	record_no,
	emp_no,
	attribute,
	value,
	eff_from_date,
	CASE
		WHEN eff_to_date &gt; ISNULL(et.Column03, '2049-01-01') THEN et.Column03
		ELSE eff_to_date 
	END AS eff_to_date 
FROM  (SELECT 'import_EMPAT' AS record_key,
			'1' AS record_no,
			ea.Column02 AS emp_no,  
			ea.Column05 AS attribute, 
			--(CASE	
			--	WHEN CONVERT(DECIMAL, ea.Column04) &gt; 0.00 THEN CONVERT(DECIMAL, ea.Column04)
			--	ELSE CONVERT(DECIMAL, ea.Column05)
			--END) 
			'Y' AS value, 
			CONVERT(DATETIME, CASE	
								WHEN ea.Column03 &lt; ed.Column07 THEN ed.Column07 
								ELSE ea.Column03 
							END) AS eff_from_date,
			CONVERT(DATETIME, ea.Column04) AS eff_to_date
		FROM gi_EMP_DETAIL ed
		INNER JOIN gi_EMP_ALLOWANCE ea ON ed.Column02 = ea.Column02
		AND	ed.Column07 &lt;= ea.Column03
		AND	ea.Column05 &lt;&gt; '' 
		AND (CONVERT(DECIMAL(5, 2), ea.Column06) &gt; 0.00 OR CONVERT(DECIMAL(5, 2), ea.Column07) &gt; 0.00)
		WHERE ed.Column10 = 'Y')A
LEFT JOIN gi_EMP_TERMINATION et ON et.Column02 = A.emp_no
WHERE eff_from_date &lt;= CASE
							WHEN eff_to_date &gt; ISNULL(et.Column03, '2049-01-01') THEN et.Column03
							ELSE eff_to_date 
						END
AND CONVERT(DATETIME, ISNULL(et.Column03, '2049-01-01')) &gt; DATEADD(MM,-12, GETDATE())</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
