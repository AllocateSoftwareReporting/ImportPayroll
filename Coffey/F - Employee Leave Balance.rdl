  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT 'import_EMPBA' AS record_key
	,'1' AS record_no
	,emlac.Column02 AS emp_no
	,emlac.Column03 AS leave_type
	,CONVERT(DATETIME, emlac.Column05) AS day_date
	,emlac.Column07 AS entitlement
	,emlac.Column06 AS pro_rata
	,'0' AS pro_rata_taken
	,CASE	
		WHEN emlac.Column04 = '' THEN '2049-01-01'
		ELSE CONVERT(VARCHAR(10), DATEADD(Year, 1, CONVERT(DATE, emlac.Column04)), 103) 
	END AS anniversary_date
FROM gi_EMP_LEAVE_BALANCE emlac
INNER JOIN gi_EMP_DETAIL emdet ON emdet.Column02 = emlac.Column02
LEFT JOIN gi_EMP_TERMINATION emter ON emter.Column02 = emlac.Column02
WHERE emlac.Column03 &lt;&gt; ''
AND (emlac.Column05 &lt;= emter.Column03 
	OR emter.Column03  IS NULL)
AND emdet.Column10 = 'Y'
AND CONVERT(DATETIME, ISNULL(emter.Column03, '2049-01-01')) &gt; DATEADD(MM,-6, GETDATE())
AND	RTRIM(emlac.Column03) NOT IN (SELECT s.payroll_ref
								FROM leave_attribute la
								INNER join shift s ON la.shift_id = s.shift_id
								WHERE	la.leave_attribute_type_id = 17
								AND	la.value = 'Y'
								)
AND	RTRIM(emlac.Column03) IN (SELECT s.payroll_ref
							FROM shift s
							WHERE s.leave_flg = 'Y'
							)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
