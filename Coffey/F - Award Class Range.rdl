  <DataSets>
    <DataSet Name="import_CLRAT">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT DISTINCT 'import_CLRAT' AS record_key,
		'1' as record_no,
		(CASE 
			WHEN slawd.Column05 = '' THEN ISNULL(a.payroll_ref, 'DEF')
			ELSE slawd.Column05 
		END) AS award,
		(CASE 
			WHEN slawd.Column05 = '' THEN ISNULL(a.payroll_ref, 'DEF')
			ELSE slawd.Column05 
		END) + '-' + slawd.Column02 as classification,
		et.payroll_ref as employment_type,
		CONVERT(DATETIME, slawd.Column03) as eff_from_date,
		CONVERT(DATETIME, slawd.Column04) as eff_to_date,
		left((cast((convert(numeric(30,5), slawd.Column07)) as varchar) + '00000'), 8) AS rate
FROM gi_award_class slawd
JOIN employment_type et ON et.employment_type_code not in ('A') 
LEFT JOIN award_classification ac ON slawd.Column02 = ac.payroll_ref
								 AND slawd.Column05 = (SELECT award_code FROM award a WHERE a.award_id = ac.award_id)
LEFT JOIN award a ON a.award_id = ac.award_id
order by classification, eff_to_date desc</CommandText>
      </Query>
